{
  "name": "vyna-ai-assistant-extension",
  "version": "1.0.0",
  "description": "Vyna AI Assistant browser extension for Chrome, Firefox, and Edge",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "build:firefox": "webpack --config webpack.firefox.config.js",
    "watch": "webpack --config webpack.config.js --watch",
    "package": "npm run build && npm run build:firefox && node scripts/package.js"
  },
  "keywords": [
    "vyna",
    "ai",
    "assistant",
    "browser-extension",
    "chrome",
    "firefox",
    "edge"
  ],
  "author": "Vyna.live",
  "license": "UNLICENSED",
  "private": true,
  "devDependencies": {
    "@types/chrome": "^0.0.262",
    "@types/firefox-webext-browser": "^115.0.0",
    "@types/react": "^18.2.71",
    "@types/react-dom": "^18.2.22",
    "archiver": "^7.0.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.10.0",
    "html-webpack-plugin": "^5.6.0",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webextension-polyfill-ts": "^0.29.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}